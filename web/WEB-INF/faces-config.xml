        </navigation-case>  
        <navigation-case>
            <from-outcome>adminTopListList</from-outcome>
            <to-view-id>/admin/toplist/topListList.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addNewTopListElement</from-outcome>
