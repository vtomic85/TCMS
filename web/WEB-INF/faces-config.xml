            <redirect/>
        </navigation-case>     
    </navigation-rule> 
    
    <!-- END OF ADMIN NAVIGATION RULES -->
    <!-- APPLICATION -->    
    <application>
        <resource-bundle>
            <base-name>locale.messages</base-name>
            <var>msgs</var>
        </resource-bundle>
        <locale-config>
            <supported-locale>en</supported-locale>
            <supported-locale>sr</supported-locale>
        </locale-config>
    </application>    
    <!-- END OF APPLICATION -->
</faces-config>
