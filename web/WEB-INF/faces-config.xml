<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <!-- BASIC NAVIGATION RULES -->
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>adminHome</from-outcome>
            <to-view-id>/admin/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>contact</from-outcome>
            <to-view-id>/contact.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>inbox</from-outcome>
            <to-view-id>/inbox/inbox.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>leaveComment</from-outcome>
            <to-view-id>/comment/commentForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>userRegistration</from-outcome>
            <to-view-id>/register/register.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>  
    <!-- END OF BASIC NAVIGATION RULES -->
    <!-- IMAGE UPLOAD NAVIGATION RULES -->
    <navigation-rule>
        <navigation-case>
            <from-outcome>uploadImageForm</from-outcome>
            <to-view-id>/admin/image/imageUploadForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>  
    <!-- END OF IMAGE UPLOAD NAVIGATION RULES -->    
    <!-- INBOX NAVIGATION RULES -->
    <navigation-rule>
        <from-view-id>/inbox/inbox.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editMessage</from-outcome>
            <to-view-id>/inbox/messageForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewMessage</from-outcome>
            <to-view-id>/inbox/messageView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inbox/messageView.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editMessage</from-outcome>
            <to-view-id>/inbox/messageForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <!-- END OF INBOX NAVIGATION RULES -->
    <!-- USER PROFILE NAVIGATION RULES -->
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>userProfilePage</from-outcome>
            <to-view-id>/user/view.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <!-- END OF USER PROFILE NAVIGATION RULES -->
    <!-- ADMIN NAVIGATION RULES -->
    <navigation-rule>
        <from-view-id>/admin/*</from-view-id>        
        <navigation-case>
            <from-outcome>adminEditItem</from-outcome>
            <to-view-id>/admin/item/itemForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>adminItemList</from-outcome>
            <to-view-id>/admin/item/itemList.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
